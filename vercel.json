{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/event/:id",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i)(facebookexternalhit|twitterbot|linkedinbot|whatsapp|telegrambot|googlebot|discord|slackbot|preview)"
        }
      ],
      "destination": "/api/og.js?type=event&id=:id"
    },
    {
      "source": "/event/:id/",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i)(facebookexternalhit|twitterbot|linkedinbot|whatsapp|telegrambot|googlebot|discord|slackbot|preview)"
        }
      ],
      "destination": "/api/og.js?type=event&id=:id"
    },
    {
      "source": "/restaurant/:id",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i)(facebookexternalhit|twitterbot|linkedinbot|whatsapp|telegrambot|googlebot|discord|slackbot|preview)"
        }
      ],
      "destination": "/api/og.js?type=restaurant&id=:id"
    },
    {
      "source": "/restaurant/:id/",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i)(facebookexternalhit|twitterbot|linkedinbot|whatsapp|telegrambot|googlebot|discord|slackbot|preview)"
        }
      ],
      "destination": "/api/og.js?type=restaurant&id=:id"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
