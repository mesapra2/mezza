{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/send-verification-sms.js": {
      "maxDuration": 10
    },
    "api/verify-phone-code.js": {
      "maxDuration": 10
    }
  },
  "env": {
    "VITE_SUPABASE_URL": "@vite_supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@vite_supabase_anon_key",
    "VITE_SITE_URL": "@vite_site_url",
    "TWILIO_ACCOUNT_SID": "@twilio_account_sid",
    "TWILIO_AUTH_TOKEN": "@twilio_auth_token",
    "TWILIO_PHONE_NUMBER": "@twilio_phone_number"
  },
  "rewrites": [
    {
      "source": "/event/:id",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i)(facebookexternalhit|twitterbot|linkedinbot|whatsapp|telegrambot|googlebot|discord|slackbot|preview)"
        }
      ],
      "destination": "/api/og.js?type=event&id=:id"
    },
    {
      "source": "/event/:id/",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i)(facebookexternalhit|twitterbot|linkedinbot|whatsapp|telegrambot|googlebot|discord|slackbot|preview)"
        }
      ],
      "destination": "/api/og.js?type=event&id=:id"
    },
    {
      "source": "/restaurant/:id",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i)(facebookexternalhit|twitterbot|linkedinbot|whatsapp|telegrambot|googlebot|discord|slackbot|preview)"
        }
      ],
      "destination": "/api/og.js?type=restaurant&id=:id"
    },
    {
      "source": "/restaurant/:id/",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i)(facebookexternalhit|twitterbot|linkedinbot|whatsapp|telegrambot|googlebot|discord|slackbot|preview)"
        }
      ],
      "destination": "/api/og.js?type=restaurant&id=:id"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
