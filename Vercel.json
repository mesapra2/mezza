{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/og.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/event/:id",
      "destination": "/api/og?event_id=:id"
    },
    {
      "source": "/restaurant/:id",
      "destination": "/api/og?partner_id=:id"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}