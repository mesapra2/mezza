{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/og.js": {
      "memory": 1024,
      "maxDuration": 15,
      "runtime": "edge"
    }
  },
  "rewrites": [
    {
      "source": "/event/:id",
      "destination": "/api/og.js?type=event&id=:id"
    },
    {
      "source": "/event/:id/",
      "destination": "/api/og.js?type=event&id=:id"
    },
    {
      "source": "/restaurant/:id",
      "destination": "/api/og.js?type=restaurant&id=:id"
    },
    {
      "source": "/restaurant/:id/",
      "destination": "/api/og.js?type=restaurant&id=:id"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
