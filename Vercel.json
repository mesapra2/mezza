{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/og.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/event/:id",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i)(facebookexternalhit|twitterbot|linkedinbot|whatsapp|telegrambot|googlebot)"
        }
      ],
      "destination": "/api/og.js?event_id=:id"
    },
    {
      "source": "/restaurant/:id",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i)(facebookexternalhit|twitterbot|linkedinbot|whatsapp|telegrambot|googlebot)"
        }
      ],
      "destination": "/api/og.js?partner_id=:id"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
