"use strict";
// src/config/userTypes.js
Object.defineProperty(exports, "__esModule", { value: true });
exports.getUserType = exports.USER_TYPES = exports.PROFILE_TYPES = void 0;
// === TIPOS DE PERFIL (para profile_type no banco) ===
exports.PROFILE_TYPES = {
    USER: 'user',
    PARTNER: 'partner',
};
// === TIPOS DE USUÁRIO (mais granulares, para lógica de negócio) ===
exports.USER_TYPES = {
    USER_FREE: 'user_free',
    USER_PREMIUM: 'user_premium',
    PARTNER_FREE: 'partner_free',
    PARTNER_PREMIUM: 'partner_premium',
};
// === FUNÇÃO PARA DETERMINAR O TIPO DE USUÁRIO ===
const getUserType = (profile) => {
    if (!profile)
        return null;
    const isPartner = profile.profile_type === exports.PROFILE_TYPES.PARTNER;
    const isPremium = profile.is_premium === true;
    if (isPartner && isPremium)
        return exports.USER_TYPES.PARTNER_PREMIUM;
    if (isPartner)
        return exports.USER_TYPES.PARTNER_FREE;
    if (isPremium)
        return exports.USER_TYPES.USER_PREMIUM;
    return exports.USER_TYPES.USER_FREE;
};
exports.getUserType = getUserType;
